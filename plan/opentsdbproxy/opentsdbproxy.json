{
  "opentsdbproxy": {
      "username": "${epu_username}",
      "groupname": "${epu_groupname}",
      "appdir": "/home/${epu_username}/opentsdbproxy",
      "port": 5252,
      "django_project_path": "/home/${epu_username}/phantomweb",
      "django_settings_module": "phantomweb.settings_nologging",
      "virtualenv": {
          "path": "/home/${epu_username}/app-venv",
          "python": "${python}",
          "virtualenv": "${virtualenv}"
      }
  },
  "recipes": [
    "opentsdbproxy"
  ]
}
